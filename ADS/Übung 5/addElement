funct addElement(char neuerWert) 
  funct addElementRek(wurzel, neuerWert)
tcnuf


funct addElementRek(TreeNode knoten, char neuerWert)
  //Neuen Koten erstellen
  TN neuerKnoten = new TN(knoten, null, null)
  if node == null
    // Im Falle wenn der Baun leer ist
    wurzel = tn
  fi
  if else neuerWert > knoten.wert
    if knoten.rechterKnoten != null
      //Laufe rechts weiter, wenn nächstes Element nicht null ist
      addElementRek(TreeNode knoten.rechterKnoten, neuerWert)
    fi
    else
      // Neuen Knoten rechts einfügen
      knoten.rechterKnoten = neuerKnoten
    esle
  fi
  if else neuerWert < knoten.wert
    if knoten.linkerKnoten != null
      //Laufe links weiter, wenn nächstes Element nicht null ist
      addElementRek(TreeNode knoten.linkerKnoten, neuerWert)
    fi
    else
      // Neuen Knoten links einfügen
      knoten.linkerKnoten = neuerKnoten
    esle
  fi
  else
    //nichts machen, da Element schon vorhanden --> Geht nicht Binärer Baum!
  esle
tcnuf
